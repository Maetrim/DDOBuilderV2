            <Type>SpellLikeAbility</Type>
            <Bonus>Enhancement</Bonus>
            <Item>Entangle</Item>
            <AType>SpellInfo</AType>
             <Amount size="3">0 0 0</Amount>
            <Item>None</Item>
         </Effect>
